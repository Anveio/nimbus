{
  "name": "@nimbus/web-demo",
  "version": "0.0.1",
  "private": true,
  "description": "A web-based terminal application to demonstrate and test the NimbusSSHWeb library.",
  "scripts": {
    "clean": "git clean -xfd .",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "lint": "biome lint .",
    "lint:fix": "biome lint --fix . && biome format --write .",
    "infra": "node infra/dev-ssh/scripts/run.mjs",
    "infra:deploy": "node infra/dev-ssh/scripts/run.mjs deploy",
    "infra:destroy": "node infra/dev-ssh/scripts/run.mjs destroy",
    "infra:synth": "node infra/dev-ssh/scripts/run.mjs synth",
    "infra:testing": "node infra/dev-ssh/scripts/run-testing.mjs",
    "infra:testing-deploy": "node infra/dev-ssh/scripts/run-testing.mjs deploy",
    "infra:testing-destroy": "node infra/dev-ssh/scripts/run-testing.mjs destroy",
    "infra:testing-synth": "node infra/dev-ssh/scripts/run-testing.mjs synth",
    "infra:testing-refresh-cache": "node infra/dev-ssh/scripts/run-testing.mjs --refresh-cache",
    "infra:cleanup-tagged": "npx tsx infra/dev-ssh/scripts/cleanup-tagged-resources.ts",
    "infra:bootstrap": "node infra/dev-ssh/scripts/bootstrap.mjs"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.4",
    "@nimbus/tui-react": "^0.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
