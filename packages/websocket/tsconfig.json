{
  "extends": "../tsconfig/workspace-package-config.json",
  "compilerOptions": {
    "customConditions": ["source"],
    "paths": {
      "@nimbus/websocket/*": ["./src/*"]
    },
    "rootDir": ".",
    "composite": false,
    "incremental": false,
    "tsBuildInfoFile": "./node_modules/.cache/websocket.tsbuildinfo"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "vitest.config.ts"],
  "exclude": ["dist", "build", "node_modules"]
}
